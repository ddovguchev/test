# Shared layout values for navbar/bar — used by Hyprland and AGS.
{
  # Outer gap (margin) around the bar — matches Hyprland gaps_out and AGS bar margin
  navbarGap = 10;
  # Bar row height (px) — AGS min-height, can be used for Hyprland reserved area
  barHeight = 32;
  # Bar corner radius (px) — rounded bar
  barRounding = 14;
}
